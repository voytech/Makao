{
    public var tittle : String;
    public var x:Integer;
    public var y:Integer;
    public var height:Integer;
    public var width:Integer;
    public var background = Rectangle {
        x: bind x,
    	y: bind y,
        width: bind width,
    	height: bind height,
        fill: Color.BLACK
