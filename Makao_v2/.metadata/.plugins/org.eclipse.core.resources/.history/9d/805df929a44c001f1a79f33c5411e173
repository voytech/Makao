package tests;

import org.junit.*; 
import static org.junit.Assert.* ;
import shared.*;
public class RequestTest {

	@Test
	public void testWhetherAfterCreatingRequestCanReturnRequestID()
	{
	//for example we can request cards named TWO.
	   for (byte id = 0; id<5;id++)
	   {
		   Request req =new Request(id,null);
		   Assert.assertEquals(id, req.getID());
	   }
	}
	//@Test
	public void testWhetherAfterCreatingRequestCanReturnRequestArgs()
	{
	//for example we can request cards named TWO.
		for (byte id = 0; id<5;id++)
		   {
			   Request req =new Request(id,null);
			   Assert.assertEquals(id, req.getID());
		   }
	}
}
