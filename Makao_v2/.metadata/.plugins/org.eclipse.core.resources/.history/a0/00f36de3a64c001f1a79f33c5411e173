package shared;

public class Request {
	 public static final byte REQUEST_FIVE = 0;
	 public static final byte REQUEST_SIX = 1;
	 public static final byte REQUEST_SEVEN = 2;
	 public static final byte REQUEST_EIGHT = 3;
	 public static final byte REQUEST_NINE = 4;
	 public static final byte REQUEST_TEN = 5;
	 public static final byte REQUEST_QUEEN = 6;
	 
	 public static final byte REQUEST_CLUBS = 7;
	 public static final byte REQUEST_DIAMOND = 8;
	 public static final byte REQUEST_HEARTS = 9;
	 public static final byte REQUEST_SPADES = 10;
	 
	 public static final byte REQUEST_WAINTING = 11;
	 public static final byte REQUEST_FOUR = 12;
	 public static final byte REQUEST_TWO = 13;
	 public static final byte REQUEST_THREE = 14;
	 
	 
	 private byte request;
	 private Object[] args = null;
     public Request(byte request, Object[] args) 
     {
		// TODO Auto-generated constructor stub
    	 this.request = request;
    	 this.args = args;
     }
     public byte getID() 
     {
		// TODO Auto-generated method stub
		return request;
     }
     public Object[] getArgs() 
     {
		// TODO Auto-generated method stub
		return args;
     }	

	
}
